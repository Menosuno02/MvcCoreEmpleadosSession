@model List<Empleado>

<h1 class="text-primary">
    Empleados Session List
</h1>

<p>
    <a asp-controller="Empleados"
       asp-action="EmpleadosAlmacenados">
        Empleados Almacenados Session
    </a>
</p>

<h2 class="text-danger">@ViewData["MENSAJE"]</h2>

<ul class="list-group">
    @foreach (Empleado emp in Model)
    {
        <li class="list-group-item list-group-item-warning">
            @emp.Apellido
            <a asp-controller="Empleados" asp-action="SessionEmpleados"
               asp-route-idempleado="@emp.IdEmpleado">
                Almacenar
            </a>
        </li>
    }
</ul>